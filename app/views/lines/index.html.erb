<h1>LINE＠</h1>

<table class="table table-striped">
  <thead>
    <tr class="bg-success text-light">
      <th scope="col">No.</th>
      <th scope="col">ジャンル</th>
      <th scope="col">内容</th>
    </tr>
  </thead>
  <tbody>
    <% @lines.each do |line| %>
      <tr>
        <th scope="row"><%= line.id %></th>
        <th scope="row"><%= "" %></th>
        <td><%= link_to line.title, line %></td>
      </tr>
    <% end %>
  </tbody>
</table>
